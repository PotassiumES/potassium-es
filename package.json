{
  "name": "potassium-es",
  "version": "0.1.0",
  "description": "A browser-side framework for wider web applications",
  "main": "src/App.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "docs": "./node_modules/.bin/esdoc",
    "precommit": "pretty-quick --staged",
    "lint": "eslint src",
    "prettier": "prettier --write src/*.js src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PotassiumES/potassium-es.git"
  },
  "keywords": [
    "webxr",
    "vr",
    "ar",
    "xr",
    "javascript-framework",
    "responsive-web"
  ],
  "author": "Trevor F. Smith <trevor@transmutable.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PotassiumES/potassium-es/issues"
  },
  "homepage": "https://github.com/PotassiumES/potassium-es#readme",
  "prettier": {
    "semi": false,
    "useTabs": true,
    "printWidth": 120
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "dependencies": {
    "action-input": "git+https://github.com/TrevorFSmith/action-input.git"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "prettier": "^1.13.7",
    "rollup": "^0.62.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  }
}
